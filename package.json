{
  "name": "indeed-data-dashboard",
  "version": "1.0.0",
  "description": "A dashboard for visualizing indeed job application data.",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "python3 -m http.server 8000 --directory src",
    "clean": "python3 scripts/clean_json.py",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "extract": "python3 scripts/extract_indeed_html.py",
    "validate": "python3 scripts/fix_json.py",
    "lint": "eslint src/**/*.js || echo 'ESLint not configured yet'"
  },
  "keywords": [
    "indeed",
    "job applications",
    "dashboard",
    "data visualization"
  ],
  "author": "Stephen",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0"
  }
}